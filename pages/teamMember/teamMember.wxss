/* pages/teamMember/teamMember.wxss */
@import "../../public/wxss/all.wxss";
.team_member {
  padding: 20rpx;
}
.list_li {
  display: flex;
  position: relative;
}
.list_li_first {
  display: flex;
  margin-bottom: 24rpx;
}

.list_li_first::after,.list_li::after {
  contain: "";
  position: absolute;
  height: 1rpx;
  background: #ddd;
}
.list_li_child_img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}
.list_li_child_nicheng {
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 24rpx;
}
/* view多选 */
.item_container {
  display: flex;
  /* flex-direction: row; */
  justify-content: space-between;
  align-items: center;
  /* padding: 20rpx; */
}
.item_container_tou {
  display: flex;
}
 
.select_icon {
  width: 40rpx;
  height: 40rpx;
}
 
.text {
  margin-left: 30rpx;
}